{
  "name": "BeInspired",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "lint": "eslint .",
    "build": "react-native bundle --entry-file ./index.js --platform ios --bundle-output ios/main.jsbundle",
    "prestart": "reset && yarn build",
    "start": "yarn test && react-native run-ios",
    "pods": "cd ios && rm Podfile.lock && pod install && cd ..",
    "postinstall": "yarn pods",
    "test": "jest --forceExit"
  },
  "dependencies": {
    "lodash": "^4.17.15",
    "react": "16.8.6",
    "react-native": "0.60.5",
    "react-native-gesture-handler": "^1.4.1",
    "react-native-linear-gradient": "^2.5.6",
    "react-navigation": "^4.0.5",
    "react-navigation-stack": "^1.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@babel/runtime": "^7.6.0",
    "@react-native-community/eslint-config": "^0.0.5",
    "babel-jest": "^24.9.0",
    "eslint": "^6.4.0",
    "jest": "^24.9.0",
    "metro-react-native-babel-preset": "^0.56.0",
    "react-test-renderer": "16.8.6"
  },
  "jest": {
    "preset": "react-native"
  }
}
